{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native Library",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "jsxImportSource": "nativewind",
    "resolveJsonModule": true,
    "target": "ESNext"
  }
}
