{
  "name": "native",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start -c",
    "dev": "expo start",
    "build": "node ./scripts/build.js",
    "prebuild": "node ./scripts/pre-build.js",
    "doctor": "pnpm dlx expo-doctor@latest",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "generate": "expo prebuild",
    "eject": "expo eject",
    "lint": "eslint .",
    "test": "jest --ci --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .expo .turbo dist node_modules"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "install": {
      "exclude": [
        "@sentry/react-native@~6.10.0",
        "@types/react@~18.2.79",
        "react@~18.3.1",
        "react-dom@~18.3.1",
        "react-native@~0.76.7",
        "react-native-gesture-handler@~2.20.2",
        "react-native-reanimated@~3.16.1",
        "react-native-webview@~13.12.5",
        "react-native-safe-area-context@~4.12.0",
        "react-native-screens@~4.4.0",
        "react-native-svg@~15.8.0",
        "typescript@~5.3.3"
      ]
    },
    "autolinking": {
      "android": {
        "buildFromSource": [
          ".*"
        ]
      },
      "exclude": [
        "expo-navigation-bar",
        "expo-status-bar"
      ]
    }
  },
  "dependencies": {
    "@legendapp/state": "3.0.0-beta.30",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@repo/bottom-sheet": "file:../../packages/bottom-sheet",
    "@repo/design": "file:../../packages/design-system",
    "@sentry/react-native": "6.14.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "5.81.5",
    "expo": "53.0.22",
    "expo-asset": "11.1.7",
    "expo-constants": "17.1.7",
    "expo-font": "13.3.2",
    "expo-image": "2.4.0",
    "expo-linking": "7.1.7",
    "expo-router": "~5.1.6",
    "expo-splash-screen": "0.30.10",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "0.28.17",
    "lodash": "4.17.21",
    "nativewind": "4.1.23",
    "numeral": "2.0.6",
    "react": "catalog:",
    "react-native": "catalog:",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "1.17.5",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "~5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "13.13.5",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@dev-plugins/react-query": "0.3.1",
    "@expo/ngrok": "4.1.3",
    "@tooling/eslint": "file:../../tooling/eslint",
    "@tooling/expo-github-cache": "file:../../tooling/egc",
    "@tooling/jest": "file:../../tooling/jest",
    "@tooling/typescript": "file:../../tooling/typescript",
    "@types/lodash": "4.17.19",
    "@types/node": "24.0.7",
    "@types/numeral": "2.0.5",
    "@types/react": "~19.0.14",
    "cross-env": "7.0.3",
    "dotenv": "17.0.0",
    "expo-build-properties": "0.14.8",
    "expo-dev-client": "5.2.4",
    "tree-kill": "1.2.2"
  }
}
